	<%
		//내장객체 : JSP에서는 Servlet에서 자주 사용되는 객체를 미리 선언
	
		out.print("Hello~");
	%>
	
	<table border="1">
		<tr>
			<%-- 아래에 있는 td태그를 10번 반복하시오 --%>
			<%for(int i=1;i<=10;i++){
				
				out.print("<td>i</td>");
			
			}%>
		</tr>
	</table>

























